<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/settings_dl"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.constraintlayout.widget.ConstraintLayout xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/settings_activity_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="true"
            android:focusableInTouchMode="true"
            tools:context=".activities.settings.SettingsActivity"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/settings_tb"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">


            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.5" />

            <Button
                android:id="@+id/theme_0_btn"
                android:layout_width="@dimen/round_btn"
                android:layout_height="@dimen/round_btn"
                android:background="@drawable/button_style_round_primary"
                android:text="accent"
                android:textAllCaps="false"
                android:textColor="?colorAccent"
                android:textSize="@dimen/small_text_size"
                android:theme="@style/AppTheme"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline"
                app:layout_constraintHorizontal_bias="0.8"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView9"
                app:layout_constraintVertical_bias="0.1" />

            <Button
                android:id="@+id/theme_2_btn"
                android:layout_width="@dimen/round_btn"
                android:layout_height="@dimen/round_btn"
                android:background="@drawable/button_style_round_primary"
                android:text="accent"
                android:textAllCaps="false"
                android:textColor="?colorAccent"
                android:textSize="@dimen/small_text_size"
                android:theme="@style/AppTheme_2"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline"
                app:layout_constraintHorizontal_bias="0.8"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/theme_0_btn"
                app:layout_constraintVertical_bias="0.209" />

            <Button
                android:id="@+id/theme_3_btn"
                android:layout_width="@dimen/round_btn"
                android:layout_height="@dimen/round_btn"
                android:background="@drawable/button_style_round_primary"
                android:text="accent"
                android:textAllCaps="false"
                android:textColor="?colorAccent"
                android:textSize="@dimen/small_text_size"
                android:theme="@style/AppTheme_3"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.2"
                app:layout_constraintStart_toStartOf="@+id/guideline"
                app:layout_constraintTop_toBottomOf="@+id/theme_1_btn"
                app:layout_constraintVertical_bias="0.209" />

            <Button
                android:id="@+id/theme_1_btn"
                android:layout_width="@dimen/round_btn"
                android:layout_height="@dimen/round_btn"
                android:background="@drawable/button_style_round_primary"
                android:text="accent"
                android:textAllCaps="false"
                android:textColor="?colorAccent"
                android:textSize="@dimen/small_text_size"
                android:theme="@style/AppTheme_1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.2"
                app:layout_constraintStart_toStartOf="@+id/guideline"
                app:layout_constraintTop_toBottomOf="@+id/textView9"
                app:layout_constraintVertical_bias="0.1" />

            <Button
                android:id="@+id/button_loans"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/square_logo"
                android:background="@drawable/button_style_round_primary"
                android:text="@string/simulate"
                android:textAllCaps="false"
                android:textColor="?textAppearanceHeadline1"
                android:textSize="@dimen/small_text_size"
                app:layout_constraintBottom_toTopOf="@+id/loan_years_it"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.623"
                app:layout_constraintStart_toStartOf="@+id/guideline"
                app:layout_constraintTop_toTopOf="@+id/loan_rate_it"
