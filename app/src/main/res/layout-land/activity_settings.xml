                android:layout_height="@dimen/round_btn"
                android:layout_marginTop="@dimen/large_margin"
                android:background="@drawable/button_style_round_primary"
                android:text="€"
                android:textColor="?textAppearanceHeadline1"
                android:textSize="@dimen/small_text_size"
                app:layout_constraintBottom_toTopOf="@+id/textView13"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.085"
                app:layout_constraintStart_toStartOf="@+id/guideline"
                app:layout_constraintTop_toBottomOf="@+id/textView10"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/amount_money_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/small_margin"
                android:text="€"
                android:textColor="?textAppearanceHeadline2"
                android:textSize="@dimen/small_text_size"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/loan_rate_it"
                app:layout_constraintStart_toEndOf="@+id/loan_amount_it"
                app:layout_constraintTop_toTopOf="@+id/loan_amount_it" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/small_margin"
                android:text="%"
                android:textColor="?textAppearanceHeadline2"
                android:textSize="@dimen/small_text_size"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/loan_rate_it"
                app:layout_constraintStart_toEndOf="@+id/loan_rate_it"
                app:layout_constraintTop_toTopOf="@+id/loan_rate_it" />

            <TextView
                android:id="@+id/textView24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/small_margin"
                android:text="@string/years"
                android:textColor="?textAppearanceHeadline2"
                android:textSize="@dimen/small_text_size"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/loan_years_it"
                app:layout_constraintStart_toEndOf="@+id/loan_years_it"
                app:layout_constraintTop_toBottomOf="@+id/loan_rate_it" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="@string/choose_a_currency"
                android:textColor="?textAppearanceHeadline2"
                android:textSize="@dimen/large_text_size"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline"
                app:layout_constraintHorizontal_bias="0.502"
                app:layout_constraintStart_toStartOf="@+id/guideline"
                app:layout_constraintTop_toBottomOf="@+id/settings_tb"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textView13"
